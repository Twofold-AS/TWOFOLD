{
  "name": "twofold",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "pnpm -w -r --parallel dev",
    "build": "pnpm -w -r build",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier -w .",
    "test": "echo \"(no tests yet)\" && exit 0",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md,css,json}": [
      "prettier -w",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.5.0"
  },
  "type": "module"
}
